{
  "name": "蓝",
  "version": "1.0",
  "manifest_version": 2,
  "description": "蓝湖生成iOS相关代码工具",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "./getPagesSource.js"
      ]
    }
  ],
  "permissions": [
    "clipboardWrite",
    "contextMenus",
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
